<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.rog//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 


<mapper namespace="com.spring.sns.*" >

	<insert id="insertUser">
	insert into snsUser (ID,NAME,PASSWORD,BIRTHDAY,SEX,EMAIL,PROFILE_IMG,SEQ) values
	(#{ID},#{NAME},#{PASSWORD},#{BIRTHDAY},#{SEX},#{EMAIL},#{PROFILE_IMG},#{SEQ})
	</insert>

</mapper>